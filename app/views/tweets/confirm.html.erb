<h3>以下の内容で送信します。</h3>

<%= form_for(@tweet) do |f| %>
    <%= f.hidden_field :content %>
    
    <P>Tweet: <%= @tweet.content %></P>
    
    <%= f.submit "Tweet" %>
<% end %>

<%= form_for(@tweet, url: new_tweet_path, method: 'get') do |f| %>
    <%= f.hidden_field :content %>
    <%= f.submit "Back", name: 'back' %>
<% end %>